<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    tools:context=".MineFragment">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="right"
            android:paddingEnd="10dp">
            <ImageView
                android:id="@+id/iv_set"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:src="#999999"
                android:layout_margin="8dp"/>
            <FrameLayout
                android:id="@+id/iv_msg"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp">
                <ImageView
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:src="#999999"
                    android:layout_margin="8dp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="#ff0000"
                    android:text="12"
                    android:layout_gravity="right"/>
            </FrameLayout>
        </LinearLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingStart="30dp"
            android:paddingEnd="30dp"
            android:paddingTop="15dp"
            android:paddingBottom="15dp">
            <ImageView
                android:id="@+id/iv_avatar"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:src="#999999"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"/>
            <TextView
                android:id="@+id/tv_name"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintEnd_toStartOf="@+id/iv_qr_code"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toEndOf="@id/iv_avatar"
                android:layout_marginLeft="20dp"
                android:singleLine="true"
                android:ellipsize="end"
                android:text="昵称"
                android:paddingBottom="5dp"
                app:layout_constraintBottom_toTopOf="@id/tv_mobile"
                android:textSize="18dp"/>
            <TextView
                android:id="@+id/tv_mobile"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintEnd_toStartOf="@+id/iv_qr_code"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@id/iv_avatar"
                android:layout_marginLeft="20dp"
                android:singleLine="true"
                android:ellipsize="end"
                android:text="手机号"
                android:paddingTop="5dp"
                app:layout_constraintTop_toBottomOf="@id/tv_name"
                android:textSize="18dp"/>
            <ImageView
                android:id="@+id/iv_qr_code"
                android:layout_width="50dp"
                android:layout_height="50dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:src="#999999"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </LinearLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="100dp">
        <LinearLayout
            android:id="@+id/ll_balance"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:gravity="center"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/ll_income">
            <TextView
                android:id="@+id/tv_balance"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="0.00"
                android:textSize="14dp"
                android:textColor="#333333"
                android:singleLine="true"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="佣金余额"
                android:textSize="14dp"
                android:textColor="#333333"
                android:singleLine="true"
                android:paddingTop="10dp"/>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/ll_income"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:gravity="center"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@id/ll_balance"
            app:layout_constraintEnd_toStartOf="@id/ll_withdrawing">
            <TextView
                android:id="@+id/tv_income"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="0.00"
                android:textSize="14dp"
                android:textColor="#333333"
                android:singleLine="true"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="累计收益"
                android:textSize="14dp"
                android:textColor="#333333"
                android:singleLine="true"
                android:paddingTop="10dp"/>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/ll_withdrawing"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:gravity="center"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@id/ll_income"
            app:layout_constraintEnd_toStartOf="@id/ll_all">
            <TextView
                android:id="@+id/tv_withdrawing"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="0.00"
                android:textSize="14dp"
                android:textColor="#333333"
                android:singleLine="true"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="提现中"
                android:textSize="14dp"
                android:textColor="#333333"
                android:singleLine="true"
                android:paddingTop="10dp"/>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/ll_all"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:gravity="center"
            app:layout_constraintEnd_toStartOf="@+id/view1"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@id/ll_withdrawing">
            <TextView
                android:id="@+id/tv_all"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="0.00"
                android:textSize="14dp"
                android:textColor="#333333"
                android:singleLine="true"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="总结单量"
                android:textSize="14dp"
                android:textColor="#333333"
                android:singleLine="true"
                android:paddingTop="10dp"/>
        </LinearLayout>
        <View
            android:id="@+id/view1"
            android:layout_width="1dp"
            android:layout_height="60dp"
            android:background="#E6E6E6"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@id/ll_all"
            app:layout_constraintEnd_toStartOf="@+id/ll_go_withdraw"/>
        <LinearLayout
            android:id="@+id/ll_go_withdraw"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:gravity="center"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@id/view1">
            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:src="#999999"
                android:singleLine="true"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="我要提现"
                android:textSize="14dp"
                android:textColor="#333333"
                android:singleLine="true"
                android:paddingTop="10dp"/>
        </LinearLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="#E6E6E6"/>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="100dp">
        <LinearLayout
            android:id="@+id/ll_pending"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:gravity="center"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/ll_rebate">
            <TextView
                android:id="@+id/tv_pending"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="0.00"
                android:textSize="14dp"
                android:textColor="#333333"
                android:singleLine="true"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="佣金余额"
                android:textSize="14dp"
                android:textColor="#333333"
                android:singleLine="true"
                android:paddingTop="10dp"/>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/ll_rebate"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:gravity="center"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@id/ll_pending"
            app:layout_constraintEnd_toStartOf="@id/ll_shipping">
            <TextView
                android:id="@+id/tv_rebate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="0.00"
                android:textSize="14dp"
                android:textColor="#333333"
                android:singleLine="true"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="累计收益"
                android:textSize="14dp"
                android:textColor="#333333"
                android:singleLine="true"
                android:paddingTop="10dp"/>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/ll_shipping"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:gravity="center"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@id/ll_rebate"
            app:layout_constraintEnd_toStartOf="@id/ll_unconfirmed">
            <TextView
                android:id="@+id/tv_shipping"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="0.00"
                android:textSize="14dp"
                android:textColor="#333333"
                android:singleLine="true"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="提现中"
                android:textSize="14dp"
                android:textColor="#333333"
                android:singleLine="true"
                android:paddingTop="10dp"/>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/ll_unconfirmed"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:gravity="center"
            app:layout_constraintEnd_toStartOf="@+id/view2"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@id/ll_shipping">
            <TextView
                android:id="@+id/tv_unconfirmed"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="0.00"
                android:textSize="14dp"
                android:textColor="#333333"
                android:singleLine="true"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="总结单量"
                android:textSize="14dp"
                android:textColor="#333333"
                android:singleLine="true"
                android:paddingTop="10dp"/>
        </LinearLayout>
        <View
            android:id="@+id/view2"
            android:layout_width="1dp"
            android:layout_height="60dp"
            android:background="#E6E6E6"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@id/ll_unconfirmed"
            app:layout_constraintEnd_toStartOf="@+id/ll_task"/>
        <LinearLayout
            android:id="@+id/ll_task"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:gravity="center"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@id/view2">
            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:src="#999999"
                android:singleLine="true"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="我要提现"
                android:textSize="14dp"
                android:textColor="#333333"
                android:singleLine="true"
                android:paddingTop="10dp"/>
        </LinearLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="#E6E6E6"/>
</LinearLayout>